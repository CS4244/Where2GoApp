; ; In destination, None is not allowed except in
; ; waterActivity and outdoorActivity.
; ; Here, a None means that the destination
; ; has a lack of water/outdoor activity
(deftemplate destination
	(slot name (type STRING))
	(slot englishSpeaking (type STRING) (default "No")(allowed-strings "None" "Yes" "No"))
	(multislot visitPreference (type STRING)(allowed-strings "None" "Undiscovered" "Romantic" "Peace and Quiet" "Cultural" "NightLife" "Family Friendly" "Leisure"))
	(slot region (type STRING)(allowed-strings "None" "North America" "Europe" "Other"))
	(multislot geography (type STRING)(allowed-strings "None" "Beach" "City" "Volcano" "Seaside" "Mountains" "Wilderness" "Island"))
	(multislot leisure (type STRING)(allowed-strings "None" "Casino" "Spa" "Shopping" "Theme Park" "Landmarks" "Zoo" "Museum" "Dining"))
	(slot daysReq (type INTEGER))
	(slot avgHotel (type INTEGER))
	(multislot activityType (type STRING)(allowed-strings  "None" "Water" "Outdoor"))
	(multislot waterActivity (type STRING)(allowed-strings "None" "Surfing" "Snorkelling" "Water Skiing" "Water Park" "Wind Surfing" "Dolphin Encounter"))
	(multislot outdoorActivity (type STRING)(allowed-strings "None" "Mountain Biking" "Rock Climbing" "Hiking" "Snow Skiing" "Zip Line" "Horseback Riding"))
	(slot weather (type STRING) (allowed-strings "None" "Hot" "Warm" "Mild" "Cold"))
	(slot expenditure (type INTEGER) (default -1))
)


; ; desired stores user's desired destination properties
; ; idea is that "None" can be used to identify unasked properties
; ; None is allowed for all slots here, as it is used to pattern match and fire questions.
(deftemplate desired
	; ;(slot name (type STRING))
	(slot englishSpeaking (type STRING) (default "None")(allowed-strings "None" "Yes" "No"))
	(multislot visitPreference (type STRING) (default "None") (allowed-strings "None" "Undiscovered" "Romantic" "Peace and Quiet" "Cultural" "NightLife" "Family Friendly" "Leisure"))
	(slot region (type STRING)(allowed-strings "None" "North America" "Europe" "Other"))
	(multislot geography (type STRING) (default "None") (allowed-strings "None" "Beach" "City" "Volcano" "Seaside" "Mountains" "Wilderness" "Island"))
	(multislot leisure (type STRING) (default "None") (allowed-strings "None" "Casino" "Spa" "Shopping" "Theme Park" "Landmarks" "Zoo" "Museum" "Dining"))
    (slot budget (type INTEGER) (default -1))
	(slot daysReq (type INTEGER) (default -1))
	(multislot activityType (type STRING) (default "None") (allowed-strings "None" "Water" "Outdoor"))
	(multislot waterActivity (type STRING) (default "None") (allowed-strings "None" "Surfing" "Snorkelling" "Water Skiing" "Water Park" "Wind Surfing" "Dolphin Encounter"))
	(multislot outdoorActivity (type STRING) (default "None") (allowed-strings "None" "Mountain Biking" "Rock Climbing" "Hiking" "Snow Skiing" "Zip Line" "Horseback Riding"))
	(slot weather (type STRING) (allowed-strings "None" "Hot" "Warm" "Mild" "Cold"))
)

(deftemplate destinationCount
	(slot count (type INTEGER) (default 0))
)

(deftemplate reportDest
(slot currStatus (type STRING) (default "running"))
(slot finalDest (type STRING) (default "None"))
)

